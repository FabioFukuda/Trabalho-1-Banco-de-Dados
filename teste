from re import T


class teste:
    def __init__(self):
        self.n =[1,2,3,4,5,6]
        self.a = 'a'
    def operacaoComNumero(func):
        def wrapper(*args,**kwargs):
            try:
                resultado = func(*args,**kwargs)
                return resultado
            except:
                return args[0].a
        return wrapper
    def func(self):
        print(self>9)
    @operacaoComNumero
    def __gt__(self,number):
        return self.a>number

    def __iter__(self):
        return iter(self.n)

    def __contains__(self, key):
        return key in self.n
    

t = teste()

print(9 in t)
