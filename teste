from re import T


class teste:
    def __init__(self):
        self.n =[1,2,3,4,5,6]
        self.a = 'a'
    def operacaoComNumero(func):
        def wrapper(*args,**kwargs):
            try:
                resultado = func(*args,**kwargs)
                return resultado
            except:
                return args[0].a
        return wrapper
    def func(self):
        print(self>9)
    @operacaoComNumero
    def __gt__(self,number):
        return self.a>number

    def __iter__(self):
        return iter(self.n)

    def __contains__(self, key):
        return key in self.n
#Com Tudo
#comando = "select s.name as nome,s.id as identificador, t.semester as semestre from student as s, takes as t where s.id = t.id and t.semester>='Spring' order by nome ASC"
#Sem Where
#comando = "select s.name as nome,s.id as identificador, t.semester as semestre from student as s, takes as t order by nome ASC"
#Sem Order by
#comando = "select s.name as nome,s.id as identificador, t.semester as semestre from student as s, takes as t where s.id = t.id and t.semester>='Spring'"
#Sem Nada
comando = "select s.name as nome,s.id as identificador, t.semester as semestre from student as s, takes as t"
comandoSeparados = comando.split(' ')
novaLista = []
_from = []
order = []

for i,comando in enumerate(comandoSeparados):
    for c in comando.split(','):
        if(c != ''):
            novaLista.append(c)
select = novaLista[1:novaLista.index('from')]
if 'where' in novaLista:
    _from = novaLista[novaLista.index('from')+1:novaLista.index('where')]
    if 'order' in novaLista:
        order = novaLista[novaLista.index('order')+2:]     
elif 'order' in novaLista:
    _from = novaLista[novaLista.index('from')+1:novaLista.index('order')]
    order = novaLista[novaLista.index('order')+2:]
else:
    _from = novaLista[novaLista.index('from')+1:]
print(select)
print(_from)
print(order)


